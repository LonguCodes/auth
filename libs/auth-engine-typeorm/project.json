{
  "name": "auth-engine-typeorm",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/auth-engine-typeorm/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/auth-engine-typeorm",
        "tsConfig": "libs/auth-engine-typeorm/tsconfig.lib.json",
        "packageJson": "libs/auth-engine-typeorm/package.json",
        "main": "libs/auth-engine-typeorm/src/index.ts",
        "assets": ["libs/auth-engine-typeorm/*.md"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs auth-engine-typeorm {args.ver} {args.tag}",
      "dependsOn": ["build"]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/auth-engine-typeorm/**/*.ts",
          "libs/auth-engine-typeorm/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/auth-engine-typeorm/jest.config.ts"
      }
    }
  },
  "tags": []
}
